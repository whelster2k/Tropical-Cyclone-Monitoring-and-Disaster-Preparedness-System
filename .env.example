# Application Settings
APP_NAME=PAGASA_Cyclone_Monitoring
APP_ENV=development
APP_DEBUG=true
APP_URL=http://localhost

# Database Configuration
DB_CONNECTION=mysql
DB_HOST=localhost
DB_PORT=3306
DB_DATABASE=pagasa_cyclone
DB_USERNAME=root
DB_PASSWORD=

# API Keys
NOAA_API_KEY=your_noaa_api_key
PAGASA_API_KEY=your_pagasa_api_key
GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Mail Configuration
MAIL_MAILER=smtp
MAIL_HOST=smtp.mailtrap.io
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS=null
MAIL_FROM_NAME="${APP_NAME}"

# SMS Gateway Configuration
SMS_PROVIDER=
SMS_API_KEY=
SMS_FROM=

# Redis Configuration
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

# Session Configuration
SESSION_DRIVER=file
SESSION_LIFETIME=120

# Security
JWT_SECRET=your_jwt_secret_key
ENCRYPTION_KEY=your_encryption_key 